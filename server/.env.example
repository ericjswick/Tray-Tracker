# Server Configuration
PORT=3000
NODE_ENV=development

# CORS Configuration
ALLOWED_ORIGINS=http://localhost:9090,http://localhost:3000,https://yourdomain.com

# Firebase Admin SDK Configuration
FIREBASE_PROJECT_ID=si-bone-tracking
FIREBASE_CLIENT_EMAIL=firebase-adminsdk-xxxxx@si-bone-tracking.iam.gserviceaccount.com
FIREBASE_PRIVATE_KEY="-----BEGIN PRIVATE KEY-----\nYOUR_PRIVATE_KEY_HERE\n-----END PRIVATE KEY-----\n"

# Database Configuration
DATABASE_URL=your-database-url-here

# API Keys
API_KEY_SECRET=your-secret-api-key-here

# Logging
LOG_LEVEL=info

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX=100

# JWT Configuration (if using JWT in addition to Firebase)
JWT_SECRET=your-jwt-secret-here
JWT_EXPIRES_IN=24h